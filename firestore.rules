rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /artifacts/interactcv/public/data/visitCounters/{docId} {
      allow read: if true;
      allow create, update, delete: if false;
    }
    match /{document=**} {
      allow read: if false;
      allow write: if false;
    }
  }
}